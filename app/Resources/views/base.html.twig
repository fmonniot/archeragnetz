<!DOCTYPE html>

<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Site Internet du club de tir à l'arc d'agnetz">
    <meta name="author" content="François Monniot">


{% block head_style %}
  {% stylesheets filter="cssembed,cssrewrite,yui_css" output="style.css"
         '../app/Resources/public/css/bootstrap.css'
         '../app/Resources/public/css/*'
  %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="all" />
  {% endstylesheets %}
{% endblock head_style %}

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <link rel="shortcut icon" href="/favicon.ico" />

    <title>{% block title %} ASA Tir à l'arc {% endblock title %}</title>
</head>

<body>
        <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="./accueil">ASA Tir à l'arc</a>
          <div class="nav-collapse collapse">
              <div class="pull-right">
                  <div class="btn-group">
                    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                    <i class="icon-user"></i> F. Monniot
                    <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu">
                    <li><a href="#">Mon profil</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Déconnexion</a></li>
                    </ul>
                  </div>
              </div>
            <ul class="nav">
              <li class="active">
                <a href="./accueil">Accueil</a>
              </li>
              <li class="">
                <a href="./vie-du-club">Vie du Club</a>
              </li>
              <li class="">
                <a href="./photos">Photos</a>
              </li>
              <li class="">
                <a href="./nous-rejoindre">Nous Rejoindre</a>
              </li>
              <li class="">
                <a href="./contact">Contact</a>
              </li>
              
            </ul>
          </div>
        </div>
      </div>
    </div>
    
<!-- Subhead
================================================== -->
<div class="jumbotron masthead">
  <div class="container">
    <img src="img/logo.png" style="float:left;margin-right:2em;height:260px;" />
    <h1>Archer d'Agnetz</h1>
  </div>
</div>
    
    <div class="container">
      {% block container %}

        <div class="footer">
            <p>
	            <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/3.0/fr/">
				    <img alt="Creative Commons License" style="border-width:0" src="http://i.creativecommons.org/l/by-nc-sa/3.0/fr/80x15.png" />
			    </a>
	    Licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/3.0/fr/">Creative Commons Attribution-NonCommercial-ShareAlike 3.0 France License</a> - Source available on <a href="http://www.github.com/francoismonniot/asacalendar/">GitHub</a>
            </p>
        </div>
      {% endblock container %}
    </div> <!-- /container -->



    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
		{% javascripts
		    '../app/Resources/public/js/libs/jquery/1.7.2/jquery.min.js'
		    '../vendor/twitter/bootstrap/js/*.js'
		    '../app/Resources/public/js/libs/wysihtml5/0.3.0/wysihtml5.js'
		    '../app/Resources/public/js/bootstrap-wysihtml5/core.js'
		    '../app/Resources/public/js/bootstrap-wysihtml5/editor.js'
		    output="script.js" filter=""
		%}
			<script type="text/javascript" src="{{ asset_url }}"></script>
		{% endjavascripts %}
</body>
</html>
